{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["prettier", "airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": 0,
    "prettier/prettier": "error",
    "no-console": 0,
    "comma-dangle": 0,
    "operator-linebreak": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-else-return": 0,
    "no-lonely-if": 0
  }
}
